{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "no-continue": "off",
        "no-unsafe-finally": "off",
        "max-len": "off",
        "class-methods-use-this": "warn"
    }
}
